student = {}

name_input = input("Enter your name: " )
age_input = input("\nEnter your age: ")
subject_input = input("\nEnter your favorite subject: ")

student['Name'] = name_input
student['Age'] = age_input
student['Favorite Subject'] = subject_input

print("\nStudent Record: ")

for key, value in student.items():
    print(f"\n{key}: {value}")
